		<fieldset id="frontend" sortorder="10">
			<field name="admin_toolbar_mode" control="enumRadioList" enum="adminToolbarModes" default="fixed" required="true" />
			<field name="disable_quick_edit" control="yesNoSwitch" />
		</fieldset>
	</tab>
</form>